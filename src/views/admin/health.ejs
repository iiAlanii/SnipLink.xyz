<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>Dashboard</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <link rel="stylesheet" href="/stylesheets/health.css">

</head>
<body>


<%- include('../partials/navbar') %>
<%- include('../partials/info') %>

<div class="main-content">
    <%- include('../partials/admin-sidebar') %>

    <div class="main-content">
        <div class="health-container">

            <div class="section intro-section">
                <h2 class="section-title">All Systems Operational</h2>
            </div>

            <div class="section">
                <h1 class="section-title">System Health</h1>
                <p class="overall-health">Overall Health: <span class="percentage"><%= overallHealth %></span></p>
                <ul class="health-list">
                    <li>API Health: <span class="percentage"><%= details.api %></span></li>
                    <li>Database Health: <span class="percentage"><%= details.database %></span></li>
                    <li>Server Health: <span class="percentage"><%= details.server %></span></li>
                </ul>
            </div>
        </div>
    </div>
</div>

</body>

</html>
